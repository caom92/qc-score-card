{
  "name": "qc-score-card-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-prod": "ng build --prod --aot=false",
    "lint": "ng lint",
    "template-build": "lessc --clean-css='--s1 --advanced --compatibility=ie8' dev/stylesheets/main.less dev/stylesheets/style.css && pug dev/web-project-canvas-frontend/templates/modals -o dev/web-project-canvas-frontend/templates/modals --hierarchy --doctype html && pug dev/templates -o dev/templates --hierarchy --doctype html",
    "template-build-win": "lessc --clean-css=\"--s1 --advanced --compatibility=ie8\" dev\\stylesheets\\main.less dev\\stylesheets\\style.css && pug dev\\templates\\ -o dev\\templates\\ --hierarchy --doctype html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": ">=4.3.3",
    "@angular/common": ">=4.0.0",
    "@angular/compiler": ">=4.0.0",
    "@angular/core": ">=4.0.0",
    "@angular/forms": ">=4.0.0",
    "@angular/http": ">=4.0.0",
    "@angular/platform-browser": ">=4.0.0",
    "@angular/platform-browser-dynamic": ">=4.0.0",
    "@angular/router": ">=4.0.0",
    "@types/plotly.js": ">=1.28.8",
    "@uirouter/angular": ">=1.0.0-beta.7",
    "angular-l10n": ">=4.1.4",
    "core-js": ">=2.4.1",
    "jquery": ">=3.2.1",
    "materialize-css": ">=0.100.1",
    "mdi": ">=2.0.46",
    "ng2-materialize": ">=1.5.1",
    "ngx-papaparse": ">=1.2.5",
    "papaparse": ">=4.3.6",
    "plotly.js": ">=1.31.2",
    "rxjs": ">=5.4.1",
    "zone.js": ">=0.8.14"
  },
  "devDependencies": {
    "@angular/cli": ">=1.2.4",
    "@angular/compiler-cli": ">=4.0.0",
    "@angular/language-service": ">=4.0.0",
    "@types/node": ">=6.0.60",
    "codelyzer": ">=3.0.1",
    "ts-node": ">=3.0.4",
    "tslint": ">=5.3.2",
    "typescript": ">=2.3.3",
    "less": ">=2.7.2",
    "less-plugin-clean-css": ">=1.5.1",
    "pug-cli": ">=1.0.0-alpha6"
  }
}
